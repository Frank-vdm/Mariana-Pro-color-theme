		"editorWidget.background": "#23292e",
		"editorSuggestWidget.background": "#ffffff20",
		
		"-- PEEK VIEW COLORS --": "Peek views are used to show references and declarations as a view inside the editor.",
		"peekViewResult.background": "#394049",
		"peekViewTitle.background": "#23292e",


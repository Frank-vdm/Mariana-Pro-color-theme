	"tokenColors": [{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#dddddd80",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#A3CE9E"
			}
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation.definition"],
			"settings": {
				"foreground": "#5fb3b3"
			}
		},
		{
			"name": "Quotes",

	"tokenColors": [
		{
			"settings": {
				"background": "#343D46",
				"foreground": "#dddddd"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#ddddddbb",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#A3CE9E"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#5fb3b3"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#FAC480"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {

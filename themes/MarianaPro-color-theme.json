		"-- NOTIFICATION COLORS --": "",
		"notificationCenterHeader.foreground": "#DDDDDD",
		"notificationCenterHeader.background": "#23292e",

		"-- QUICK PICKER --": "",
		"pickerGroup.border":"#6699cc",
		"pickerGroup.foreground":"#6699cc"


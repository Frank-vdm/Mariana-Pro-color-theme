{
    "name": "mariana-pro",
    "displayName": "Mariana Pro",
    "description": "A complete color theme for VS Code based on Sublime Text's Mariana Syntax and Adaptive UI",
    "version": "2.2.1",
    "publisher": "rickynormandeau",
    "repository": "https://github.com/n0rmand0/Mariana-Pro-color-theme",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "Sublime Text",
        "Mariana",
        "Adaptive UI",
        "Color Theme",
        "Theme"
    ],
    "icon": "icon.png",
    "contributes": {
        "themes": [
            {
                "label": "Mariana Pro (Cool)",
                "uiTheme": "vs-dark",
                "path": "./themes/MarianaPro-color-theme.json"
            },
            {
                "label": "Mariana Pro (Warm)",
                "uiTheme": "vs-dark",
                "path": "./themes/MarianaPro-warm-color-theme.json"
            },
            {
                "label": "Mariana Pro (Gray)",
                "uiTheme": "vs-dark",
                "path": "./themes/MarianaPro-gray-color-theme.json"
            }
        ]
      },
    "dependencies": {
        "gulp": "^3.9.1",
        "gulp-exports2json": "^0.1.1"
    },
    "scripts": {
        "build": "gulp"
    }
}
